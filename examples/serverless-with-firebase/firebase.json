{
  "hosting": {
    "predeploy": [
      "yarn build:public"
    ],
    "public": "dist/public",
    "ignore": [],
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [{
      "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
      "headers": [{
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }]
    }, {
      "source": "**/*.@(jpg|jpeg|gif|png)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=7200"
      }]
    }],
    "rewrites": [
      {
        "source": "/login",
        "function": "LoginPage"
      },
      {
        "source": "**/**",
        "function": "IndexPage"
      }
    ]
  },
  "functions": {
    "predeploy": [
      "yarn clean",
      "yarn build"
    ],
    "source": "dist/functions"
  }
}
