{
  "compilerOptions": {
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "target": "es3",
    "lib": ["dom"],
    "downlevelIteration": true,
    "preserveWatchOutput": true,
    "outDir": "lib",
    "jsx": "react",
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "declaration": true
  }
}
