{
  "NOTION_KEY": {
    "description": "Notion API key",
    "required": true
  },
  "DATABASE_SECRET": {
    "description": "Postgres database password",
    "required": true,
    "env": {
      "development": {
        "defaultValue": "abc"
      },
      "production": {
        "defaultValue": "cba"
      }
    }
  },
  "APP_TITLE": {
    "description": "The title of the current App",
    "required": true,
    "defaultValue": "hello"
  },
  "SENTRY_DSN": {
    "description": "Our Sentry DSN for tracking oops",
    "required": true,
    "env": {
      "test": { "required": false },
      "development": { "required": false }
    }
  }
}
